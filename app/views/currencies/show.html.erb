<p id="notice"><%= notice %></p>

<div class="row">   
  <span class="col-sm-12 col-md-4">
    <strong>Currency:</strong>
    <%= @currency.name %>
  </span>
</div>
<br>
<div class="row">
  <span class="col-sm-6 col-md-2">
    <%= button_to 'Edit', edit_currency_path(@currency), method: 'get' %>
  </span>
  <span class="col-sm-6 col-md-2">
    <%= button_to 'Back', currencies_path, method: 'get' %>
  </span>
</div>