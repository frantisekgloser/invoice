<p id="notice"><%= notice %></p>

<div class="row">   
  <span class="col-sm-6 col-md-3">
    <strong>Sequence:</strong>
    <%= @insequence.name %>
  </span>
  <span class="col-sm-6 col-md-3">
    <strong>Note:</strong>
    <%= @insequence.note %>
  </span>
</div>
<br>
<div class="row">
  <span class="col-sm-6 col-md-2">
    <%= button_to 'Edit', edit_insequence_path(@insequence), method: 'get' %>
  </span>
  <span class="col-sm-6 col-md-2">
    <%= button_to 'Back', insequences_path, method: 'get' %>
  </span>
</div>